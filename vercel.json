{
    "version": 2,
    "builds": [
        {
          "src": "prisma/*",
          "use": "@vercel/prisma"
        }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
         ]
      }
    ]
  }